{
  "name": "iot-platforma",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "src/modules/*"
  ],
  "scripts": {
    "devFE": "node scripts/start.js",
    "build": "node scripts/build.js",
    "testttttttt": "node scripts/test.js",
    "test": "env ROOT_PATH=${PWD}/public mocha ./src/modules/backend/test ./src/modules/framework-ui/test ./src/modules/framework/test --require @babel/register",
    "mochaBackend": "env ROOT_PATH=${PWD}/public mocha ./src/modules/backend/test --require @babel/register",
    "mochaFrontend": "env ROOT_PATH=${PWD}/public mocha ./src/modules/frontend/test",
    "mochaFramework-ui": "env ROOT_PATH=${PWD}/public mocha ./src/modules/framework-ui/test --require @babel/register",
    "mochaFramework": "env ROOT_PATH=${PWD}/public mocha ./src/modules/framework/test --require @babel/register",
    "devBE": "nodemon --watch src/modules/backend --exec env ROOT_PATH=${PWD}/public babel-node ./src/modules/backend/index.js ",
    "devBE-mqtt": "nodemon --watch src/modules/backend --watch src/modules/backend-mqtt --exec babel-node ./src/modules/backend-mqtt/index.js",
    "buildBE": "env ROOT_PATH=${PWD}/public babel src/modules -d distBE",
    "server1": "env ROOT_PATH=${PWD}/public node distBE/backend/index.js",
    "server2": "env ROOT_PATH=${PWD}/public node distBE/backend-mqtt/index.js",
    "serveFE": "env ROOT_PATH=${PWD}/public cp -r build/* \"$ROOT_PATH\""
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "@svgr/webpack": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "babel-preset-react-app": "^7.0.1",
    "bcrypt": "^3.0.6",
    "bfj": "6.1.1",
    "body-parser": "^1.18.3",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "cors": "^2.8.5",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "^7.0.1",
    "helmet": "^3.16.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "jsonwebtoken": "^8.5.0",
    "key-mirror": "^1.0.1",
    "mini-css-extract-plugin": "0.5.0",
    "mongodb": "^3.3.1",
    "mongoose": "^5.4.18",
    "morgan": "^1.9.1",
    "mqtt": "^3.0.0",
    "node-cmd": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "ramda": "^0.26.1",
    "ramda-extension": "^0.7.2",
    "react": "^16.8.3",
    "react-app-polyfill": "^0.2.1",
    "react-dev-utils": "^7.0.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.8.0",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "framework-ui*": "<rootDir>/framework-ui"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "/Users/martas/Work/projects/iot-platforma/frontend/node_modules/jest-watch-typeahead/filename.js",
      "/Users/martas/Work/projects/iot-platforma/frontend/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "devDependencies": {
    "hat": "^0.0.3",
    "mocha": "^6.2.0",
    "should": "^13.2.3",
    "supertest": "^4.0.2"
  },
  "homepage": "https://test.iotplatforma.cloud",
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "500",
    "env": {
      "NODE_ENV": "development"
    }
  }
}
